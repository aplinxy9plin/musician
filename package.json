{
  "name": "musician",
  "version": "1.0.2",
  "description": "Standalone music player for VK",
  "main": "main.js",
  "build": {
    "appId": "com.uenify.musician",
    "mac": {
      "category": "public.app-category.music",
      "target": "dmg",
      "icon": "icon.icns",
      "asar": "false"
    },
    "win": {
      "target": "nsis"
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uenify/musician.git"
  },
  "keywords": [
    "VKAPI",
    "Vk",
    "music",
    "player",
    "electron"
  ],
  "author": "Uenify",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uenify/musician/issues"
  },
  "homepage": "https://github.com/uenify/musician#readme",
  "devDependencies": {
    "electron": "^1.6.10",
    "electron-builder": "^19.37.2"
  },
  "dependencies": {
    "electron-settings": "^3.1.3",
    "node-notifier": "^5.1.2"
  }
}
